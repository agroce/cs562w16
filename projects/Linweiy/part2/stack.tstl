@import stack

#test the availability of general function(add,remove,is_empty)


pools:
        <stack> 3
        <int> 10

actions:
        <int> := <[0..20]>      
        <stack> := stack.Stack()
        ~<stack>.is_empty() and True
        ~<stack>.add(~<int>) => <stack,1>.size() == len(<stack,1>.stack_list)
        (~<stack>.is_empty() and False) -> ~<stack,1>.remove()
