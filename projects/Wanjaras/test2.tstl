@import stack


pools:
        <stack> 3
        <data> 12


actions:
        <data> := <[0..0]>      
        <stack> := stack.Stack()
        ~<stack>.add(~<data>) => (<data,1> in <stack,1>.stack_list) \
        and (<stack,1>.size() == (pre<(<stack,1>.size())>+1))
        (not ~<stack>.is_empty()) -> l = <stack,1>.stack_list; val = l[len(l)-1];\
        remv = ~<stack,1>.remove() => val == remv
